<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.example.org/Config"
	xmlns:tns="http://www.example.org/Config" elementFormDefault="qualified">
	
	<xs:element name="Config">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="IP" type="xs:string" />
				<xs:element name="Port" type="xs:positiveInteger" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	
</xs:schema>